							"});",
							"    ",
							"",
							"pm.test(\"Your test name\", function () {",
							"    pm.expect(pm.response.text()).to.include(\"Google Pixel 6 Pro\");",
							"});",
							"",
							"var appleProducts = [];",
							"",
							"for (let i = 0; i < response.length; i++) {",
							"    if (response[i].name.includes(\"Apple\") ) {",
							"        appleProducts.push(response[i].id);",
							"        }",
							"};",
							"",
							"console.log(\"Якийсь текст\", appleProducts);",
							"",
							"pm.test(\"Correct amount of Apple products\", () => {",
							"    pm.expect(appleProducts.length).to.eql(10);",
